        {
            "nombrePersonaje": "Sr. Tostada",
            "texto": "Jaja ¿eso fue una victoria reñida o aplastante?",
            "imagenPersonajePath": "Sprites/mr_tostada",
            "personajeHablando": true
        },
        {
            "nombrePersonaje": "Mr. Pato",
            "texto": "Bueno eso realmente no importa mi amigo, fue divertido mientras duro.",
